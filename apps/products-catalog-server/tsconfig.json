{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@controllers/*": ["./src/controllers/*"],
      "@elasticsearch/*": ["./src/elasticsearch/*"],
      "@middlewares/*": ["./src/middlewares/*"],
      "@routes/*": ["./src/routes/*"],
      "@schemas/*": ["./src/schemas/*"],
      "@models/*": ["src/models/*"]
    },
    "types": ["node"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "outDir": "./dist"
  },
  "exclude": ["node_modules"],
  "extends": "@niki/typescript-config/base.json",
  "include": ["**/*.ts", "src/domain/models/product.model.ts"]
}
